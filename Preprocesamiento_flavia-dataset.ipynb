   "source": [
    "def convert_to_one_hot(Y, C):\n",
    "    Y = np.eye(C)[Y.reshape(-1)].T\n",
    "    return Y\n",
    "\n",
    "y_oh = convert_to_one_hot(y_array,32).T\n",
    "print (y_oh[2])\n"
