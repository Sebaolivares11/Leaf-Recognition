    "# separamos los datos en X = entrada e y = salida\n",
    "for features, label in training_data:\n",
    "    X.append(features)\n",
    "    y.append(label)\n",
    "    \n",
    "X_array = np.array(X).reshape(-1,IMG_SIZE,IMG_SIZE,3)\n",
    "y_array = np.array(y)\n",
